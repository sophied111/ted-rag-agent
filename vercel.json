{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/prompt",
      "dest": "/api/prompt.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
